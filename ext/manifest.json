{
  "manifest_version": 2,
  "name": "Emo - you need more emoji in your life",
  "description": "Translates English text into emojis",
  "version": "0.7",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": true,
      "scripts": [
      "translatelib.js",
      "background.js"
      ]
  }
}
